<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hy.mapper.ms.SysDictMapper">
    <resultMap id="BaseResultMap" type="com.hy.model.SysDict" >
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="code" property="code" jdbcType="VARCHAR" />
        <result column="name" property="name" jdbcType="VARCHAR" />
        <result column="pid" property="pid" jdbcType="INTEGER" />
        <result column="deleteFlag" property="deleteFlag" jdbcType="TINYINT" />
        <result column="creater" property="creater" jdbcType="VARCHAR" />
        <result column="created" property="created" jdbcType="TIMESTAMP" />
        <result column="modifier" property="modifier" jdbcType="VARCHAR" />
        <result column="modified" property="modified" jdbcType="TIMESTAMP" />
    </resultMap>
    <sql id="Base_Column_List" >
        id, code, name, pid, deleteFlag, creater, created, modifier, modified
    </sql>
    <select id="selectQzgzNewsTypeId" resultMap="BaseResultMap">
        select <include refid="Base_Column_List" />
        from sys_dict
        where code = 'qzgz_news_type'
    </select>
    <select id="selectQzgzNewsType" resultMap="BaseResultMap" parameterType="java.lang.Integer">
        select <include refid="Base_Column_List" />
        from sys_dict
        where pid = #{pid} and deleteFlag = 0
    </select>
    <insert id="insertQzgzNewsType" parameterType="com.hy.model.SysDict">
        insert into sys_dict (name, pid, creater, modifier)
        values (#{name}, #{pid}, #{creater}, #{modifier})
    </insert>
    <update id="updateQzgzNewsType" parameterType="com.hy.model.SysDict">
        update sys_dict
        set name = #{name}, modifier = #{modifier}
        where id = #{id}
    </update>
    <update id="deleteQzgzNewsType" parameterType="java.lang.Integer">
        update sys_dict
        set deleteFlag = 1
        where id = #{id}
    </update>
</mapper>