<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hy.mapper.ms.QzgzAdminMapper">
    <select id="selectAdminByUsername" resultType="com.hy.model.QzgzAdmin">
        SELECT * FROM qzgz_admin
        WHERE username = #{username}
    </select>
    <update id="updateToken">
        UPDATE qzgz_admin
        SET token = #{token}
        WHERE username = #{username}
    </update>
    <select id="selectToken" resultType="java.lang.Integer">
        SELECT count(*) FROM qzgz_admin
        WHERE token = #{token}
    </select>
</mapper>