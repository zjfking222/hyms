<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hy.mapper.ms.SalesmanTracerMapper">
    <resultMap id="BaseResultMap" type="com.hy.model.SalesmanTracer">
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="salesmannum" property="salesmannum" jdbcType="VARCHAR" />
        <result column="salesmanname" property="salesmanname" jdbcType="VARCHAR" />
        <result column="tracernum" property="tracernum" jdbcType="VARCHAR" />
        <result column="tracername" property="tracername" jdbcType="VARCHAR" />
        <result column="creater" property="creater" jdbcType="VARCHAR" />
        <result column="created" property="created" jdbcType="TIMESTAMP" />
        <result column="modifier" property="modifier" jdbcType="VARCHAR" />
        <result column="modified" property="modified" jdbcType="TIMESTAMP" />
    </resultMap>
    <sql id="Base_Column_List" >
        id, salesmannum, salesmanname, tracernum, tracername, creater, created, modifier, modified
    </sql>
    <select id="selectSalesmanTracer" resultMap="BaseResultMap" parameterType="java.lang.String">
        select <include refid="Base_Column_List" />
        from salesman_tracer
        where salesmannum = #{salesmannum}
    </select>
    <select id="selectDistinctSalesman" resultType="com.hy.model.SalesmanTracer" parameterType="java.lang.String">
        select distinct salesmannum, salesmanname
        from salesman_tracer
        where 1=1
        <if test="value != null and '' != null">
            and (salesmannum = #{value} or salesmanname = #{value})
        </if>
    </select>
    <insert id="insertSalesmanTracer" parameterType="com.hy.model.SalesmanTracer">
        insert into salesman_tracer
        (salesmannum, salesmanname, tracernum, tracername, creater, modifier)
        values
        <foreach collection="list" item="item" separator=",">
            (#{item.salesmannum}, #{item.salesmanname}, #{item.tracernum}, #{item.tracername}, #{item.creater},
            #{item.modifier})
        </foreach>
    </insert>
    <delete id="deleteSalesmanTracer" parameterType="java.lang.Integer">
        delete from salesman_tracer
        where id in
        <foreach collection="array" item="item" open="(" separator="," close=")">
            #{item}
        </foreach>
    </delete>
    <delete id="deleteBySalesmannum" parameterType="java.lang.String">
        delete from salesman_tracer
        where salesmannum = #{salesmannum}
    </delete>
</mapper>