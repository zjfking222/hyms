<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hy.mapper.ms.CanteenMapper">

    <select id="selectCanteen" resultType="com.hy.model.Canteen">
      SELECT id,name,price
      FROM qzgz_canteen
      WHERE state = #{state}
    </select>
</mapper>