<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hy.mapper.ms.QzgzCanteenMapper">

    <select id="selectCanteen" resultType="com.hy.model.QzgzCanteen">
      SELECT *
      FROM qzgz_canteen
      WHERE 1=1
      <if test="state != null and '' != state">
          AND state = #{state}
      </if>
      LIMIT #{startRow},#{number}
    </select>

    <select id="selectCountOfCanteen" resultType="java.lang.Integer">
        SELECT COUNT(*)
        FROM qzgz_canteen
        WHERE 1=1
        <if test="state != null and '' != state">
            AND  state = #{state}
        </if>
    </select>
    
    <select id="selectAllCanteen" resultType="com.hy.model.QzgzCanteen">
        SELECT *
        FROM qzgz_canteen
    </select>

</mapper>